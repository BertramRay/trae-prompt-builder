{
  "name": "trae-prompt-builder",
  "displayName": "trae prompt builder",
  "description": "Generate detailed trae AI code editor prompt with simple words!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "dependencies": {
    "openai": "^4.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "trae-prompt-builder.generatePrompt",
        "title": "Trae: Generate AI Prompt"
      }
    ],
    "configuration": {
      "title": "Trae Prompt Builder",
      "properties": {
        "trae-prompt-builder.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API Key for generating prompts"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
